<h1><%= t('.page_header') %> "{{term}}"</h1>

<h3><%=t(".borrow_heading")%></h3>

<p>{{items}}</p>
<table class="table table-striped table-hover" ng-show="searchdata.items.length > 0">
	
  <tbody>
    <tr ng-repeat="item in searchdata.items" title="{{item.description}}">
      <td><img ng-src="{{item.image_path}}" class="img-rounded" width="40"></td>
      <td><strong>{{item.data.name}}</strong></td>
      <td>{{item.distance}} <%=t(".km_away")%></td>
      <td>
        <button type="button" class="btn btn-success btn-sm pull-right">
        <span class="glyphicon glyphicon-send"></span> <%=t(".send_request")%></button>
      </td>
    </tr>
  </tbody>
</table>
<div class="alert alert-warning" ng-show="searchdata.items.length===0"><%=t(".no_items_found")%></div>


